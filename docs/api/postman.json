  "item": [
    {
      "name": "Health",
      "item": [
        {
          "name": "Health Check",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/health",
              "host": ["{{base_url}}"],
              "path": ["health"]
            },
            "description": "Returns the current health status of the BIOMETRICS API. No authentication required."
          },
          "response": [
            {
              "name": "Healthy",
              "status": "OK",
              "code": 200,
              "body": "{\n  \"status\": \"healthy\",\n  \"version\": \"1.0.0\",\n  \"uptime\": 86400,\n  \"timestamp\": \"2026-02-19T10:30:00Z\"\n}"
            }
          ]
        }
      ]
    },
    {
      "name": "Agents",
      "item": [
        {
          "name": "List All Agents",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const jsonData = pm.response.json();",
                  "    if (jsonData.agents && jsonData.agents.length > 0) {",
                  "        pm.collectionVariables.set('agent_id', jsonData.agents[0].id);",
                  "    }",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/agents?status=active&limit=20&offset=0",
              "host": ["{{base_url}}"],
              "path": ["agents"],
              "query": [
                {
                  "key": "status",
                  "value": "active",
                  "description": "Filter by agent status (active, idle, busy, offline)"
                },
                {
                  "key": "limit",
                  "value": "20",
                  "description": "Maximum number of agents to return"
                },
                {
                  "key": "offset",
                  "value": "0",
                  "description": "Number of agents to skip"
                }
              ]
            },
            "description": "Returns a list of all available AI agents in the swarm"
          },
          "response": []
        },
        {
          "name": "Create Agent",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('agent_id', jsonData.id);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Custom-Agent\",\n  \"role\": \"coder\",\n  \"model\": \"qwen/qwen3.5-397b-a17b\",\n  \"config\": {}\n}"
            },
            "url": {
              "raw": "{{base_url}}/agents",
              "host": ["{{base_url}}"],
              "path": ["agents"]
            },
            "description": "Creates a new AI agent in the swarm"
          },
          "response": []
        },
        {
          "name": "Get Agent Details",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/agents/{{agent_id}}",
              "host": ["{{base_url}}"],
              "path": ["agents", "{{agent_id}}"]
            },
            "description": "Returns detailed information about a specific agent"
          },
          "response": []
        },
        {
          "name": "Delete Agent",
          "request": {
            "method": "DELETE",
            "header": [],
            "url": {
              "raw": "{{base_url}}/agents/{{agent_id}}",
              "host": ["{{base_url}}"],
              "path": ["agents", "{{agent_id}}"]
            },
            "description": "Removes an agent from the swarm"
          },
          "response": []
        },
        {
          "name": "List Agent Tasks",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/agents/{{agent_id}}/tasks?status=in_progress",
              "host": ["{{base_url}}"],
              "path": ["agents", "{{agent_id}}", "tasks"],
              "query": [
                {
                  "key": "status",
                  "value": "in_progress",
                  "description": "Filter by task status (pending, in_progress, completed, failed)"
                }
              ]
            },
            "description": "Returns all tasks assigned to a specific agent"
          },
          "response": []
        }
      ]
